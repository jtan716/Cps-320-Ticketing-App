import requests

test1 = {"rowid": "A", "colid": 1} #invalid eventlinkid

test2 = {"rowid": "2", "colid": 1} #rowid is not a letter 

test3 = {"rowid": "a", "colid": 1} #rowid is not a captial letter 

test5 = {"rowid": "A", "colid": "a"} #colid is not a number 

test4 = {"rowid": "A", "colid": 1} #duplicate seat

r1 = requests.post("http://localhost:5000/events/0/seating",json=test1)
print(f'Response Code: {r1.status_code}\n{r1.text}') #Seat must be for an existing event.

r2 = requests.post("http://localhost:5000/events/1/seating",json=test2) #Row indexes must be capital letters only.
print(f'Response Code: {r2.status_code}\n{r2.text}')

r3 = requests.post("http://localhost:5000/events/1/seating",json=test3) #Column indexes must be whole numbers and non-zero.
print(f'Response Code: {r3.status_code}\n{r3.text}')\

r5 = requests.post("http://localhost:5000/events/1/seating",json=test5) #Column indexes must be whole numbers and non-zero.
print(f'Response Code: {r5.status_code}\n{r5.text}')

r4 = requests.post("http://localhost:5000/events/1/seating",json=test4) #Column indexes must be whole numbers and non-zero.
print(f'Response Code: {r4.status_code}\n{r4.text}')